#22.
dicti={
    2:20,
    3:30,
    5:50,
    4:40,
    1:10}
x=int(input())
if x in dicti:
    print(x,' dict-д байна.')
else:
    print(Байхгүй байна'')

#23. 
dict3 = { 
    '2':20,
    '3':30,
    '5':50,
    '4':40,
    '1':10 } 
x=int(input())
if x in dict3.values():
    print("Байна")
else:
    print("Байхгүй байна")


