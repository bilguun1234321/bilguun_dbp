#3
def urj(k):
    b = 1
    for i in k:
        n = n * i
    return n

numbers=[1,2,3,4,5,6]
print(urj(numbers))

#4. 
def urj(k):
    n=1
    for i in k:
        n=k[2]*k[-1]
    return n

numbers=[1,2,3,4,5,6]
print(urj(numbers))

#5.
def max(k):
    a=max(k)
    return a
def min(k):
    b=min(k)
    return b

numbers=[1,2,3,4,5,6]
print(max(numbers),min(numbers))

#6.
x=['ababa', 'dahs', '4224']
s=0
for i in x:
    if len(i)>2 and i[0]==i[-1]:
        s=s+1
print(s)

#7.
def davkhardsan(a):
    return list(dict.fromkeys(a))
list = davkhardsan(['bbbbcb', 'abcd', 411, 111, 'abcd'])
print(list) 

#8.
def empty(a):
    if len(a):
        print('Хоосон биш')
    else:
        print('Хоосон')
a=['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h']
empty(a)    

