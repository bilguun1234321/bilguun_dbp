#13.
given_tuple = ('a', 'b', 'c', 'd', 'e', 'f', 'g', 'h')
char = input('Enter a character to find: ')
for ch in given_tuple:
    if ch == char:
        print('It is in the tuple')
        break
    else:
        print('It is not in the tuple')
        break

